RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f


RewriteRule ^catalog/([-a-zA-Z0-9_]+)$ catalog.php?category_name=$1 [L]
RewriteRule ^catalog/([-a-zA-Z0-9_]+)/([-a-zA-Z0-9_]+)-([a-zA-Z]+)$ product.php?category_name=$1&product_url_name=$2&color_name=$3 [L]